<%= link_to campaign_votes_path(
  vote: {
    voted_picture_id: @campaign.picture_a_id
  },
  campaign_id: @campaign.id),
  method: :post do %>
  <%= image_tag Picture.find(@campaign.picture_a_id).file %>
<% end %>

<%= link_to campaign_votes_path(
  vote: {
    voted_picture_id: @campaign.picture_b_id,
  },
  campaign_id: @campaign.id),
  method: :post do %>
  <%= image_tag Picture.find(@campaign.picture_b_id).file %>
<% end %>